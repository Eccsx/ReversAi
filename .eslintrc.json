{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "warn",
    "import/extensions": "warn",
    "class-methods-use-this": "warn"
  },
  "plugins": [
    "jest"
  ]
}